<body onload="loadfinish()" style="font-family:Microsoft JhengHei;">
    <div class=" container text-center">
        <div class="card p-3">
            <div class="card-header">
                <h3>審查頁面</h3>
            </div>
        </div>
    </div>
    <div aria-disabled="true" class="d-flex mt-3">

        <iframe onload="frameload()" style="margin-left: 2%;" id="needReviewForm" width="66%" height="600px"
            frameborder="1" scrolling="yes"></iframe>
        <iframe onload="reviewload()" style="margin-left: 2%;margin-right: 2%;" id="ReviewForm" width="33%"
            height="600px" frameborder="1" scrolling="yes"></iframe>

    </div>
    <div aria-disabled="true" class="d-flex mt-3">
        <button id="saveBt" type="submit" onclick="saveBtClick()" class="btn btn-secondary m-auto col-md-2">暫存</button>
        <button id="submitBt" type="submit" onclick="submitBtClick()" class="btn btn-info m-auto col-md-2">提交</button>
        <button id="canselBt" type="button" onclick="backBtClick()" class="btn btn-danger m-auto col-md-2">取消</button>
    </div>
    <script src="./committeeReview.js"></script>
</body>